<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Marriage-age-validation</display-name>
  <servlet>
    <servlet-name>ValidationServlet</servlet-name>
    <servlet-class>Person_data.ValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidationServlet</servlet-name>
    <url-pattern>/ValidationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EligibilityServlet</servlet-name>
    <servlet-class>Person_data.EligibilityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EligibilityServlet</servlet-name>
    <url-pattern>/EligibilityServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>person.html</welcome-file>
  </welcome-file-list>
</web-app>